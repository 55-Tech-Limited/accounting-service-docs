+  private getBetTrailDescriptionFromStatus(offerStatus: string): string {
+    switch (offerStatus) {
+      case "requesting":
+        return "Requesting for bet";
+      case "accepting":
+        return "Waiting to accept bet offer";
+      case "accepted":
